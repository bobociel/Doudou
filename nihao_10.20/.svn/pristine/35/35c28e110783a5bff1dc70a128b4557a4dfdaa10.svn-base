#import <Foundation/Foundation.h>

@interface Picture : NSObject

/*! 图像id*/
@property (assign, nonatomic) NSInteger picture_id;
/*! 图像大图url*/
@property (copy,nonatomic) NSString *picture_big_network_url;
/*! 图像原图url*/
@property (nonatomic, copy) NSString *picture_original_network_url;
/*! 图像小图url*/
@property (nonatomic, copy) NSString *picture_small_network_url;
/*! 图像缩略图url*/
@property (nonatomic, copy) NSString *picture_thumbnail_network_url;
/*! 小图的长度*/
@property (nonatomic,assign) NSInteger picture_small_width;
/*! 小图的宽度*/
@property (nonatomic,assign) NSInteger picture_small_height;
/*! 缩略图的长度*/
@property (nonatomic,assign) NSInteger picture_thumbnail_width;
/*! 缩略图的宽度*/
@property (nonatomic,assign) NSInteger picture_thumbnail_height;

@end
