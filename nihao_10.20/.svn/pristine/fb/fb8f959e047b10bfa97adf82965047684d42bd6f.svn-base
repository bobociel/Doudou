#import <Foundation/Foundation.h>

@interface ChatHistoryModel : NSObject

@property (nonatomic, strong) NSString *username;
@property (nonatomic, strong) NSString *time;

@property (nonatomic, strong) NSString *message;

@property (nonatomic, assign) MessageBodyType messageType;
@property (nonatomic, assign) CGSize imageSize;
@property (nonatomic, assign) CGSize thumbnailSize;

/*本地图片路径*/
@property (nonatomic, strong) NSString *imageFile;      //本地大图路径
@property (nonatomic, strong) NSString *thumbnailFile;  //本地小图路径

/*网络图片路径*/
@property (nonatomic, strong) NSString *imageRemote;     //网络大图路径；
@property (nonatomic, strong) NSString *trumbnailRemote; //网络小图路径；

@property (nonatomic, strong) NSString *massageID;
@end
