#import <Foundation/Foundation.h>

@interface NihaoContact : NSObject

/*! 用户id*/
@property (nonatomic, assign) NSInteger ci_id;

/*! 用户昵称*/
@property (nonatomic, copy) NSString *ci_nikename;

/*! 用户头像url*/
@property (nonatomic, copy) NSString *ci_header_img;

/*! 用户性别 0为女，1为男*/
@property (nonatomic, assign) NSInteger ci_sex;

/*! 用户所在城市名*/
@property (nonatomic, copy) NSString *city_name_en;

/*! 用户所在国籍*/
@property (nonatomic, copy) NSString *country_name_en;

/*! 用户名，聊天使用*/
@property (nonatomic, copy) NSString *ci_username;

/*! 用户类型*/
@property (nonatomic, assign) NSUInteger ci_type;

/*! 不看他的动态*/
@property (nonatomic, assign) NSInteger not_look_her;

/*! 不让他看我的动态*/
@property (nonatomic, assign) NSInteger her_not_look_me;

@end
